{
	"name": "multiline-singleline",
	"displayName": "Multi-Line to Single-Line",
	"description": "Convert multi line code to single line so as to fit within a string",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.56.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:multiline-singleline.Multi-Line to Single-Line"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "multiline-singleline.Multi-Line to Single-Line",
				"title": "Multi-Line to Single-Line"
			}
		],
		"keybindings": [
			{
			  "command": "multiline-singleline.Multi-Line to Single-Line",
			  "key": "ctrl+alt+1",
			  "mac": "cmd+alt+1",
			  "when": "editorTextFocus"
			}
		  ]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.56.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"eslint": "^7.27.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	}
}
