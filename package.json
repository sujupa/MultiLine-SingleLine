{
	"name": "multiline-singleline",
	"displayName": "MultiLine to SingleLine",
	"description": "Convert text/code of multiple lines into single line",
	"version": "0.0.2",
	"publisher":"SujayPatil",
	"icon":"images/icon.png",
	"author": {
		"name": "Sujay Patil"
	},
	"engines": {
		"vscode": "^1.56.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:multiline-singleline.MultiLine to SingleLine"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "multiline-singleline.MultiLine to SingleLine",
				"title": "MultiLine to SingleLine"
			}
		],
		"keybindings": [
			{
			  "command": "multiline-singleline.MultiLine to SingleLine",
			  "key": "ctrl+alt+1",
			  "mac": "cmd+alt+1",
			  "when": "editorTextFocus"
			}
		  ]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.56.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"eslint": "^7.27.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/sujupa/MultiLine-SingleLine"
	  },
	  "homepage": "https://github.com/sujupa/MultiLine-SingleLine/blob/master/README.md"
}
